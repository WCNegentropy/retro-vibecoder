{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capability for the main window - full Tauri desktop app with procedural and template generation",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "core:window:allow-close",
    "core:window:allow-minimize",
    "core:window:allow-maximize",
    "core:window:allow-unmaximize",
    "core:window:allow-is-maximized"
  ]
}
